<!DOCTYPE html>
<html lang="ja">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}

        {% endblock title %}</title>
    <link href="https://unpkg.com/lightbox2@2.11.4/dist/css/lightbox.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="../static/css/style.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- jQueryの追加 -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- Slickの追加 -->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
</head>

<body>
    <header>
        <div class="header">
            <div class="container">
                <h1>{% block FV_title %}

                    {% endblock FV_title %}</h1>
                <img class="left_img" src="../static/images/wave1.png" alt="">
            </div>

            <div class="right">
                <nav class="nav">
                {% block nav %}
                <a href="#about">
                    <p>about</p>
                </a>
                <a href="#works">
                    <p>works</p>
                </a>
                <a href="./contact.html">
                    <p>contact</p>
                </a>
                {% endblock nav %}
                   
                </nav>
                {% block right_img %}
                <img class="right_img" src="{{ url_for('static', filename='./images/me_child.png') }}"
                    alt="me_in_child">
                {% endblock right_img %}

            </div>

        </div>
    </header>
    <main>
        <div class="main1" id="about">
            {% block main1 %}
            <img src="{{ url_for('static',filename='images/me_adlt.png') }}" alt="me_in_festival">
            <div class="text">
                <p></p>
                <h1></h1>
                <p></p>
            </div>
            {% endblock main1 %}
        </div>
        {% block main2 %}
        <div class="main2" id="works">
            <h1>{% block main2_title %}

                {% endblock main2_title %}</h1>

            {% block works_content %}
            <div class="works">
                <a href="{{ url_for('migake') }}" class="image-container"><img
                        src="{{ url_for('static', filename='images/Login.png') }}" alt=""></a>
                <a href="{{ url_for('precompany') }}" class="image-container"><img
                        src="{{ url_for('static',filename='images/Top.png') }}" alt=""></a>
                <a href="#" class="image-container"><img src="../img/schoolfecworks.png" alt="preparing"></a>
            </div>
            <div class="works">
                <a href="#" class="image-container"><img src="../img/Top.png" alt="preparing"></a>
                <a href="#" class="image-container"><img src="../img/Drnature.png" alt="preparing"></a>
                <a href="#" class="image-container"><img src="../img/1nennseisakubutu.png" alt="preparing"></a>
            </div>
            {% endblock works_content %}</div>
        {% endblock main2 %}
        
            

        </div>
        </div>
    </main>
    <footer>
    {% block footer %}
        <a href="#" class="up">
            <p>top</p>
        </a>
        <a href="#about" class="about">
            <p>about</p>
        </a>
        <a href="#works" class="work_footer">
            <p>works</p>
        </a>
        <a href="./contact.html" class="contact">
            <p>contact</p>
        </a>
    {% endblock footer %}
        
    </footer>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.7.1/js/lightbox.min.js"></script>
    <script src="../static/js/my_script.js"></script>
</body>

</html>